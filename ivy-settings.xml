<ivysettings>
	<settings defaultResolver="chained" />
	<resolvers>
		<chain name="chained" returnFirst="true">
			<url name="steelhouse">
				<ivy pattern="http://repo001/repos/ivy/[organisation]/[module]/[revision]/ivy.xml" />
				<artifact pattern="http://repo001/repos/ivy/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]" />
			</url>
			<ibiblio name="twttr.com" m2compatible="true" root="http://maven.twttr.com" />
			<ibiblio name="repo.codahale.com" m2compatible="true" root="http://repo.codahale.com"/>
			<ibiblio name="ibiblio" m2compatible="true" />
		</chain>
	</resolvers>
</ivysettings>
